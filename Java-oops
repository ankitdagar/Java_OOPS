 package oops;

class Cat
{
	boolean hasfur;
	String colour , breed1;
	String breed;
	int eyes, legs;
	
public void walk()
    {
	System.out.println("cat is walking");
	}
public void eat()      
    {
	System.out.println("cat is eating"); 
	}
public void description()
    { 
	System.out.println("my cat has "+legs+" legs and " + eyes + " eyes" + breed1);
    }
}
class Dog
{
	String breed,name;
public void  jump() 
	{	
	System.out.println("My dog "+ name +" Jumped ");
	}
public void description() 
	{	
	System.out.println("My dog's name is "+ name +" & breed is "+ breed); 
	}
}

public class MainClass {

	public static void main(String[] args) {
        Cat cat1=new Cat();
        Cat cat2 =new Cat();
        cat1.legs=3;
        cat2.eyes=2;
        cat1.eyes=2;
        cat2.legs=3;
        cat1.legs=3;
        cat1.breed1="billi";
        
         cat1.description();
         cat2.description();

	}

}
